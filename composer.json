{
    "name": "lav45/yii2-translated-behavior",
    "description": "This extension allows you to quickly and simple enough to add translations for any ActiveRecord models.",
    "keywords": [
        "yii2",
        "extension",
        "behavior",
        "translated"
    ],
    "homepage": "https://github.com/lav45/yii2-translated-behavior",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Alexey Loban",
            "email": "lav451@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2-dev": "^3.0@dev"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "4 - 5",
        "phpunit/dbunit": "*",
        "ext-sqlite3": "*"
    },
    "autoload": {
        "psr-4": {
            "lav45\\translate\\": "src",
            "lav45\\translate\\test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
